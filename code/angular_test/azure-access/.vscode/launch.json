{
        "version": "0.2.0",
        "configurations": [
            {
                "name": "Launch Chrome against localhost, with sourcemaps",
                "type": "chrome",
                "request": "launch",
                "url": "http://localhost:9000",
                "sourceMaps": true,
                "trace": true,
                "webRoot": "${workspaceRoot}",
                "userDataDir": "${workspaceRoot}/.vscode/chrome"
            }  
        ]
    }